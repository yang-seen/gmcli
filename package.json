{
	"name": "@willtmc/gmcli",
	"version": "0.2.0",
	"description": "Minimal Gmail CLI (fork with bug fixes)",
	"repository": {
		"type": "git",
		"url": "https://github.com/willtmc/gmcli.git"
	},
	"type": "module",
	"bin": {
		"gmcli": "dist/cli.js"
	},
	"main": "./dist/gmail-service.js",
	"types": "./dist/gmail-service.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc && chmod +x dist/cli.js",
