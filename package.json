		"prepare": "husky"
	},
	"dependencies": {
		"google-auth-library": "^10.1.0",
		"googleapis": "^153.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^22.10.1",
		"@typescript/native-preview": "^7.0.0-dev.20251204.1",
		"husky": "^9.1.7",
		"typescript": "^5.7.2"
	},
	"keywords": [
		"gmail",
		"email",
		"cli"
	],
	"author": "",
	"license": "MIT",
	"engines": {
		"node": ">=20.0.0"
	}
}
